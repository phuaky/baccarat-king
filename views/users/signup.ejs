<h1>SIGN UP</h1>

<form class="ui form" id="submit-signup" action="/users/signup" method="POST">
    <div class="field">
      <label>Email</label>
      <input type="email" name="email" placeholder="gambler@huat.ah">
    </div>
    <div class="field">
      <label>Username</label>
      <input type="text" name="username" placeholder="Lucky Strike">
    </div>
    <div class="field">
      <label>Password</label>
      <input type="password" name="password" placeholder="#8#8#8">
    </div>
  <button class="ui button" type="submit">Submit</button>
</form>
